			protected function User_dragDropHandler(event:DragEvent):void
			{
				trace(A_m_AC.length);
				if(Object(event.dragInitiator).id != 'A_m_DG'){
					var o:* = event.dragSource.dataForFormat('items');
					var c:int = 0;
					for (var i:int = 0; i < event.currentTarget.dataProvider.length; i++) 
					{
						if(o[0].UserId == event.currentTarget.dataProvider[i].UserId){
							c++;
						}
					}
					
					if(c == 0 ){
						event.currentTarget.dataProvider.addItem(o[0]);						
					} else {
						event.preventDefault();
//						Alert.show('item Duplicat');
						//Alert.show('User is already a member of the group or role');
					}
				}
			}
			
			protected function GroupRole_dragDropHandler(event:DragEvent):void
			{
				var o:Array = event.dragSource.dataForFormat('items') as Array;
				var c:int = 0;
				var chkdup:Boolean = false;
				var ac:ArrayCollection = new ArrayCollection();
				
				for (var j:int = 0; j < o.length; j++) 
				{	
					c = 0;
					for (var i:int = 0; i < event.currentTarget.dataProvider.length; i++) 
					{
						if(o[j].UserId == event.currentTarget.dataProvider[i].UserId){
							c++;
						} 
					}
					
					if(c == 0 ){
						ac.addItem(o[j]);
					} else {
						chkdup = true;
//						event.preventDefault();
					}
				}
				
				if(chkdup && ac.length == 0){
					Alert.show("User already a member of the group or role.");
					event.preventDefault()
				}
				
				if(chkdup && ac.length > 0 ){
					Alert.show("One or more users are already members of the group or role.  Do " +
						"you want to add the users that are not " +
						"duplicates? ",'Warning',Alert.YES|Alert.NO,this,function(e:CloseEvent):void {
						if(e.detail == Alert.YES){
							for (var k:int = 0; k < ac.length; k++) 
							{
								event.currentTarget.dataProvider.addItem(ac[k]);	
							}
							
						} else {
							event.preventDefault();
						}
					});
				} else {
					
					for (var k:int = 0; k < ac.length; k++) 
					{
						event.currentTarget.dataProvider.addItem(ac[k]);							
					}
				}
			}
			
			protected function dropEnter(event:DragEvent):void
			{
				if(Object(event.dragInitiator).id == 'A_m_DG' && event.dragSource.hasFormat('items')){
					var dropTarget:AdvancedDataGrid = AdvancedDataGrid(event.currentTarget);// as dataGrid
					DragManager.acceptDragDrop(dropTarget);
				} else {
					if(Object(event.currentTarget).id == 'A_m_DG'){
						var dropTarget:AdvancedDataGrid = AdvancedDataGrid(event.currentTarget);// as dataGrid
						DragManager.acceptDragDrop(dropTarget);
					}
				}
			}
			
