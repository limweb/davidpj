				RightDateBox.width=0; RightDateBox.includeInLayout= false;
				
				if (RightSymbolBox.numElements > 0)
				{
					RightSymbolBox.removeAllElements();
				}
				RightSymbolBox.width=0; RightSymbolBox.includeInLayout= false;
				
				CharactersAdded = 0;
				CenterSymbol = false;
				
				LeftLoad = 0;
				RightLoad = 0;
				
				HideShow();
				CalculatePrice();
			}
			
