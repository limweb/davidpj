					m_VS.includeInLayout = true;
					m_VS.visible = true;
				}
				if (CategoryAlias == "WX") 
				{
					//Blank the stage
					ClearAll();
				}
				if (CategoryAlias == "WP")
				{
					TB.dataProvider = WP_TB_AC;
					DG_TW.title = 'Procedures / Plans';
					m_VS.selectedChild = WP_m_view;
				}
				if (CategoryAlias == "WP" && CategoryView == "Main")
				{
					WP_m_view.percentWidth = DGpercentwidth;
					WP_m_view.includeInLayout = true;
					WP_m_view.visible = true;
					WP_m_DG.width = DGwidth;
					WP_m_DG.percentHeight = 100;
					WP_m_DG.horizontalScrollPolicy ='on';
