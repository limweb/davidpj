					DG_TW.title = 'Forms';
				}
				if (CategoryAlias == "WF" && CategoryView == "Main")
				{
					H2.percentWidth = (DGpercentwidth/2);
					H2.includeInLayout = true;
					H2.visible = true;
					HDB.setStyle('horizontalGap',0);
					H1.percentWidth = (DGpercentwidth/2);
					H1.includeInLayout = true;
					H1.visible = true;
					m_VS.percentWidth = 50;
					m_VS.selectedChild = WF_m_view;
					WF_m_view.percentWidth = 50;
					WF_m_view.includeInLayout = true;
					WF_m_view.visible = true;
					WF_m_DG.width = DGwidth;
					W_d_VS.percentWidth = 50;
					W_d_VS.includeInLayout = true;
					W_d_VS.visible = true;
					H1_VS.selectedChild = WF_Preview_view;
					WF_Preview_view.percentWidth = 100;
					WF_Preview_view.includeInLayout = true;
					WF_Preview_view.visible = true;
				}
				if (CategoryAlias == "WF" && CategoryView == "Fields")
				{					
					H2.percentWidth = (DGpercentwidth/2);
					H2.includeInLayout = true;
					H2.visible = true;
					HDB.setStyle('horizontalGap',5);
					H3.percentWidth = (DGpercentwidth/2);
					H3.includeInLayout = true;
					H3.visible = true;
					m_VS.selectedChild = FF_m_view;
					FF_m_view.percentWidth = (DGpercentwidth/2);
					FF_m_view.includeInLayout = true;
					FF_m_view.visible = true;
					Reset_FF_m_DG();
					FF_m_DG_formusage1.visible = false;
					FF_m_DG_formusage2.visible = false;
					F_d_VS.selectedChild = FF_Fieldsets_view;
					FF_Fieldsets_view.width = (DGwidth/2)
					FF_Fieldsets_DG.width = (DGwidth/2);
					FF_Fieldsets_DG.includeInLayout = true;
					FF_Fieldsets_DG.visible = true;
				}
				if (CategoryAlias == "WF" && CategoryView == "Preview")
				{		
					HDB.setStyle('horizontalGap',5);
					H1.percentWidth = (DGpercentwidth/3);
					H1.includeInLayout = true;
					H1.visible = true;
					H2.percentWidth = (DGpercentwidth/3);
					H2.includeInLayout = true;
					H2.visible = true;
					H3.percentWidth = (DGpercentwidth/3);
					H3.includeInLayout = true;
					H3.visible = true;
					m_VS.selectedChild = FF_m_view;
					FF_m_view.percentWidth = (DGpercentwidth/2);
					FF_m_view.includeInLayout = true;
					FF_m_view.visible = true;
					Reset_FF_m_DG();
					FF_m_DG_formusage1.visible = false;
					FF_m_DG_formusage2.visible = false;
					F_d_VS.selectedChild = FF_Fieldsets_view;
					FF_Fieldsets_view.width = (DGwidth/2)
					FF_Fieldsets_DG.width = (DGwidth/2);
					FF_Fieldsets_DG.includeInLayout = true;
					FF_Fieldsets_DG.visible = true;
				}
				if (CategoryAlias == "WQ")
				{
					DG_TW.title = 'Queries';
					//Blank the stage
					ClearAll();
				}
				if (CategoryAlias == "WC")
				{
					DG_TW.title = 'Scripts';
					//Blank the stage
					ClearAll();
				}
				if (CategoryAlias == "WE")	
				{
					DG_TW.title = 'Events';
					//Blank the stage
					ClearAll();
				}
				if (CategoryAlias == "WM")	
				{
					DG_TW.title = 'Messages';
					//Blank the stage
					ClearAll();
				}
				if (CategoryAlias == "J" && CategoryView == "Main")
				{
					DG_TW.title = 'Jobs';
					TB.visible = false;
					m_VS.selectedChild = J_m_view;
					m_VS.percentWidth = DGpercentwidth;
					J_m_view.percentWidth = DGpercentwidth;
					J_m_view.includeInLayout = true;
					J_m_view.visible = true;
					J_m_DG.width = DGwidth;
					J_m_DG.percentHeight = 100;
				}
				if (CategoryAlias == "S" && CategoryView == "Main")
				{
					DG_TW.title = 'System';
					m_VS.selectedChild = S_m_view;
					TB.visible = false;
					m_VS.percentWidth = DGpercentwidth;
					S_m_view.percentWidth = DGpercentwidth;
					S_m_view.includeInLayout = true;
					S_m_view.visible = true;
					S_m1_DG.width = DGwidth;
					S_m1_DG.percentHeight = 100;
					S_m2_DG.width = DGwidth;
					S_m2_DG.percentHeight = 100;
					S_m3_DG.width = DGwidth;
					S_m3_DG.percentHeight = 100;
				}
			}
			
