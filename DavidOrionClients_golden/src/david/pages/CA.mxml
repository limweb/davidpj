	<fx:Script>
		<![CDATA[
			import flash.events.MouseEvent;
			
			import mx.collections.ArrayCollection;
			import mx.controls.Alert;
			import mx.events.IndexChangedEvent;
			import mx.events.ListEvent;
			
			import spark.events.IndexChangeEvent;
			
			[Bindable]
			public var DGwidth:Number = 1157;
			public var DGpercentwidth:Number = 100;
			public var DGID:String;
			
			public var CategoryAlias:String
			public var CategoryAliasGroup:String
			public var CategoryView:String
			
			protected function canvas1_creationCompleteHandler():void 
			{
				CategoryAlias = "AU";
				CategoryAliasGroup = "A";
				CategoryView = "Main";
				SetCAView(CategoryAlias, CategoryView);
			}
			
			protected function ClearAll():void 
			{
				m_VS.width = 0;
				m_VS.includeInLayout = false;
				m_VS.visible = false;
				ClearDetail();
			}

			protected function ClearDetail():void
			{
				F_d_VS.width = 0;
				F_d_VS.includeInLayout = false;
				F_d_VS.visible = false;
				W_d_VS.width = 0;
				W_d_VS.includeInLayout = false;
				W_d_VS.visible = false;
			}
			
			protected function CategoryMenuChangeHandler(event:ListEvent):void
			{
				var CategoryItem:Object = event.target.selectedItem as Object;
				
				CategoryAlias = CategoryItem.Alias;
				CategoryAliasGroup = CategoryItem.Alias.substring(0, 1);
				CategoryView = "Main";

				SetCAView(CategoryAlias, CategoryView);
			}
			
			protected function TabBarChangeHandler(event:IndexChangeEvent):void
			{
				var TB_Data:Object = event.target.selectedItem as Object;
				
				if (CategoryAlias == "WP" && TB_Data.Data == "CRUD") {
					CategoryView = "CRUD";
				}			
				else if (CategoryAlias == "WP" && TB_Data.Data == "Forms") {
					CategoryView = "Forms";
				}
				else if (CategoryAlias == "FF" && TB_Data.Data == "FormUsage1") {
					CategoryView = "FormUsage1";
				}
				else if (CategoryAlias == "FF" && TB_Data.Data == "FormUsage2") {
					CategoryView = "FormUsage2";
				}
				else if (CategoryAlias == "FF" && TB_Data.Data == "Fieldsets") {
					CategoryView = "Fieldsets";
				}
				
				SetCAView(CategoryAlias, CategoryView);
			}

			protected function SetCAView(CategoryAlias:String, CategoryView:String):void
			{
				if (CategoryView == "Main") {
					H2.percentWidth = DGpercentwidth;
					H2.includeInLayout = true;
					H2.visible = true;
					H3.percentWidth = 0;
					H3.includeInLayout = false;
					H3.visible = false;
					HDB.setStyle('horizontalGap',0);
					m_VS.percentWidth = DGpercentwidth;
					m_VS.includeInLayout = true;
					m_VS.visible = true;
					ClearDetail();
				}
				if (CategoryAliasGroup == "A")
				{
					//Account Management
					TB.visible = true;
					m_VS.width = DGwidth;
					m_VS.includeInLayout = true;
					m_VS.visible = true;
				}
				if (CategoryAlias == "AX")
				{
					//Blank the stage
					ClearAll();
				}
				if (CategoryAlias == "AU")
				{
					TB.dataProvider = AU_TB_AC;
					DG_TW.title = 'Users';
					m_VS.selectedChild = AU_m_view;
					AU_m_view.includeInLayout = true;
					AU_m_view.visible = true;
					Reset_AU_m_DG();
				}
				if (CategoryAlias == "AU" && CategoryView == "Main")
				{
					AU_m_view.percentWidth = 100;
					AU_m_DG.width = DGwidth;
					AU_m_DG.percentHeight = 100;
					AU_m_DG.horizontalScrollPolicy ='off';
					AU_m_DG_Property3.width = 570;
					AU_m_DG_propertiesB.visible = true;
					AU_m_DG.horizontalScrollPolicy ='off';
				}
				if (CategoryAlias == "AU" && CategoryView == "Contact")
				{	
					/*A_d_VS.percentWidth = 0;
					A_d_VS.includeInLayout = false;
					A_d_VS.visible = false;
					A_s_VS.percentWidth = 0;
					A_s_VS.includeInLayout = false;
					A_s_VS.visible = false;*/
				}
				if (CategoryAlias == "AG")
				{
					TB.dataProvider = AG_TB_AC;
					DG_TW.title = 'Groups';
					m_VS.selectedChild = AG_m_view;
					AG_m_view.includeInLayout = true;
					AG_m_view.visible = true;
					Reset_AG_m_DG();
				}
				if (CategoryAlias == "AG" && CategoryView == "Main")
				{
					AG_m_view.percentWidth = 100;
					AG_m_DG.width = DGwidth;
					AG_m_DG.percentHeight = 100;
					AG_m_DG.horizontalScrollPolicy ='off';
					AG_m_DG_Property3.width = 570;
					AG_m_DG_propertiesB.visible = true;
					AG_m_DG.horizontalScrollPolicy ='off';
				}
				if (CategoryAlias == "AR")
				{
					TB.dataProvider = AR_TB_AC;
					DG_TW.title = 'Roles';
					m_VS.selectedChild = AR_m_view;
					AR_m_view.includeInLayout = true;
					AR_m_view.visible = true;
					Reset_AR_m_DG();
				}
				if (CategoryAlias == "AR" && CategoryView == "Main")
				{
					AR_m_DG.width = DGwidth;
				}
				if (CategoryAliasGroup == "D")
				{
					//Document management
					TB.visible = true;
					m_VS.width = DGwidth;
					m_VS.includeInLayout = true;
					m_VS.visible = true;
				}
				if (CategoryAlias == "DX")
				{
					//Blank the stage
					ClearAll();
				}
				if (CategoryAlias == "DT")
				{
					TB.dataProvider = DT_TB_AC;
					DG_TW.title = 'Document Types';
				}
				if (CategoryAlias == "DT" && CategoryView == "Main")
				{
					m_VS.selectedChild = DT_m_view;
					DT_m_view.includeInLayout = true;
					DT_m_view.visible = true;
					Reset_DT_m_DG();
				}
				if (CategoryAliasGroup == "F")
				{
					//Fieldsets/Fields and Folders
					TB.visible = true;
				}
				if (CategoryAlias == "FF")
				{
					TB.dataProvider = FF_TB_AC;
					DG_TW.title = 'Fieldsets/Fields';
				}
				if (CategoryAlias == "FF" && CategoryView == "Main")
				{
					m_VS.selectedChild = FF_m_view;
					FF_m_view.percentWidth = DGpercentwidth;
					FF_m_view.includeInLayout = true;
					FF_m_view.visible = true;
					Reset_FF_m_DG();
					FF_m_DG_formusage1.visible = false;
					FF_m_DG_formusage2.visible = false;
				}
				if (CategoryAlias == "FF" && CategoryView == "FormUsage1")
				{
					m_VS.selectedChild = FF_m_view;
					FF_m_view.percentWidth = DGpercentwidth;
					FF_m_view.includeInLayout = true;
					FF_m_view.visible = true;
					Reset_FF_m_DG();
					FF_m_DG_formusage1.visible = true;
					FF_m_DG_formusage2.visible = false;
					
				}
				if (CategoryAlias == "FF" && CategoryView == "FormUsage2")
				{
					m_VS.selectedChild = FF_m_view;
					FF_m_view.percentWidth = DGpercentwidth;
					FF_m_view.includeInLayout = true;
					FF_m_view.visible = true;
					Reset_FF_m_DG();
					FF_m_DG_formusage1.visible = false;
					FF_m_DG_formusage2.visible = true;
				}
				if (CategoryAlias == "FF" && CategoryView == "Fieldsets")
				{
					H2.percentWidth = (DGpercentwidth/2);
					H2.includeInLayout = true;
					H2.visible = true;
					HDB.setStyle('horizontalGap',5);
					H3.percentWidth = (DGpercentwidth/2);
					H3.includeInLayout = true;
					H3.visible = true;
					m_VS.selectedChild = FF_m_view;
					FF_m_view.percentWidth = (DGpercentwidth/2);
					FF_m_view.includeInLayout = true;
					FF_m_view.visible = true;
					Reset_FF_m_DG();
					FF_m_DG_formusage1.visible = false;
					FF_m_DG_formusage2.visible = false;
					F_d_VS.selectedChild = FF_Fieldsets_view;
					FF_Fieldsets_view.width = (DGwidth/2)
					FF_Fieldsets_DG.width = (DGwidth/2);
					FF_Fieldsets_DG.includeInLayout = true;
					FF_Fieldsets_DG.visible = true;
				}
				if (CategoryAlias == "FD")
				{
					TB.dataProvider = FD_TB_AC;
					DG_TW.title = 'Folders';
				}
				if (CategoryAlias == "FD" && CategoryView == "Main")
				{	
