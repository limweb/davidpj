				F_d_VS.includeInLayout = false; F_d_VS.visible = false;
				W_d_VS.includeInLayout = false; W_d_VS.visible = false;
			}
			
			protected function CategoryMenuChangeHandler(event:ListEvent):void
			{
				var CategoryItem:Object = event.target.selectedItem as Object;
				
				//Save last-used category to var to detect change of category
				LastCategoryAlias = CategoryAlias;

				CategoryAlias = CategoryItem.Alias;
				CategoryAliasGroup = CategoryItem.Alias.substring(0, 1);
				CategoryView = "Main";

				SetCAView(CategoryAlias, CategoryView);
			}
			
			protected function TabBarChangeHandler(event:IndexChangeEvent):void
			{
				var TB_Data:Object = event.target.selectedItem as Object;
				
				// If category changes, select first tabbar tab
				if (CategoryAlias != LastCategoryAlias)
				{
					TB.selectedIndex = 0;
				}
				
				if (CategoryAlias == "AU" && TB_Data.Data == "ID") //User ID
				{
					CategoryView = "ID";
				}
				else if (CategoryAlias == "AU" && TB_Data.Data == "P1") //Properties...
				{
					CategoryView = "P1";
				}
				else if (CategoryAlias == "AU" && TB_Data.Data == "CO") //Companies
				{
					CategoryView = "CO";
				}
				else if (CategoryAlias == "AU" && TB_Data.Data == "AC") //Access controls
				{
					CategoryView = "AC";
				}
				else if (CategoryAlias == "FF" && TB_Data.Data == "FormUsage1") 
				{
					CategoryView = "FormUsage1";
				}
				else if (CategoryAlias == "FF" && TB_Data.Data == "FormUsage2") 
				{
					CategoryView = "FormUsage2";
				}
				else if (CategoryAlias == "FF" && TB_Data.Data == "Fieldsets") 
				{
					CategoryView = "Fieldsets";
				}
				if (CategoryAlias == "WF" && TB_Data.Data == "ID") //Form ID
				{
					CategoryView = "ID";
				}
				if (CategoryAlias == "WF" && TB_Data.Data == "FF") //Form fields
				{
					CategoryView = "FF";
				}
				if (CategoryAlias == "WF" && TB_Data.Data == "PF") //Plan/Procedure fields
				{
					CategoryView = "PF";
				}
				if (CategoryAlias == "WF" && TB_Data.Data == "FP") //Form preview
				{
					CategoryView = "FP";
				}
				else if (CategoryAlias == "WP" && TB_Data.Data == "CRUD") 
				{
					CategoryView = "CRUD";
				}			
				else if (CategoryAlias == "WP" && TB_Data.Data == "Forms") 
				{
					CategoryView = "Forms";
				}

				SetCAView(CategoryAlias, CategoryView);
			}
