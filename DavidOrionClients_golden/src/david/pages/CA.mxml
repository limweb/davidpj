					WP_m_DG.horizontalScrollPolicy ='off';
				}
				if (CategoryAlias == "WP" && CategoryView == "CRUD")
				{
					ClearAll();
					WP_m_view.percentWidth = (DGpercentwidth/2);
					WP_m_view.includeInLayout = true;
					WP_m_view.visible = true;
					W_d_VS.percentWidth = (DGwidth/2);
					W_d_VS.includeInLayout = true;
