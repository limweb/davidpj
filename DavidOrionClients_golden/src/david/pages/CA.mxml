			protected function canvas1_creationCompleteHandler():void 
			{
				CategoryAlias = "AU";
				CategoryAliasGroup = "A";
				CategoryView = "Main";
				SetCAView(CategoryAlias, CategoryView);
			}
			
			protected function ClearAll():void 
			{
				m_VS.includeInLayout = false; m_VS.visible = false;
				sm1_VS.includeInLayout = false; sm1_VS.visible = false;
				sm2_VS.includeInLayout = false; sm2_VS.visible = false;
				ClearDetail();
			}
			
			protected function ClearMaster():void
			{
				m_VS.includeInLayout = false; m_VS.visible = false;
				sm1_VS.includeInLayout = false; sm1_VS.visible = false;
				sm2_VS.includeInLayout = false; sm2_VS.visible = false;
			}

			protected function ClearDetail():void
			{
