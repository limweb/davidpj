			private function ChangeCurrentBox():void
			{
				if (TwoLines == false)
				{
					if (CurrentBox == 'Box1A')
					{
						CurrentBox = 'Box2A';
						Box1B.includeInLayout=false; Box1B.visible=false;
						Box2.includeInLayout=true; Box2.visible=true;
						Box2A.includeInLayout=true; Box2A.visible=true;
						Box2B.includeInLayout=false; Box2B.visible=false;
						CurrentLine = 'Line 1';
					}
					else if (CurrentBox == 'Box1B')
					{
						CurrentLine = 'Line 2';
					}
				}
				else if (TwoLines == true)
				{
					if (CurrentBox == 'Box1A')
					{
						CurrentBox = 'Box1B';
						Box1B.includeInLayout=true; Box1B.visible=true;
						CurrentLine = 'Line 2';
					}
					if (CurrentBox == 'Box1B')
					{
						CurrentBox = 'Box2A';
						Box2.includeInLayout=true; Box2.visible=true;
						Box2A.includeInLayout=true; Box2A.visible=true;
						Box2B.includeInLayout=true; Box2B.visible=true;
						CurrentLine = 'Line 1';
					}
					if (CurrentBox == 'Box2A')
					{
						CurrentBox = 'Box2B';
						Box2B.includeInLayout=true; Box2B.visible=true;
						CurrentLine = 'Line 2';
					}
					if (CurrentBox == 'Box2B')
					{
						CurrentLine = 'Line 2';
					}
				}
				HideShowOutlines();
			}
			
			private function HideShowOutlines():void
			{
				if (CurrentBox =='Box1A')
				{
					Box1AOutline.visible=true;
					Box1BOutline.visible=false;
					Box2AOutline.visible=false;
					Box2BOutline.visible=false;
				}
				else if (CurrentBox =='Box1B')
				{
					Box1AOutline.visible=false;
					Box1BOutline.visible=true;
					Box2AOutline.visible=false;
					Box2BOutline.visible=false;
				}
				else if (CurrentBox =='Box2A')
				{
					Box1AOutline.visible=false;
