			private function SelectFoilGalleryImage(event:MouseEvent):void 
			{
				FoilIsImage = true;
				var FoilGalleryImage:Image;
				FoilGalleryImage = event.currentTarget as Image;
				SelectedFoilImage = FoilGalleryImage;
				
				if (SelectedFoilImage.indexOf("Black") >= 0)
				{
					FoilName = "BlackFoil";
				}
				else if (SelectedFoilImage.indexOf("White") >= 0)
				{
					FoilName = "WhiteFoil";
				}
				else if (SelectedFoilImage.indexOf("Blue") >= 0)
				{
					FoilName = "BlueFoil";
				}
				else if (SelectedFoilImage.indexOf("Gold") >= 0)
				{
					FoilName = "GoldFoil";
				}
				else if (SelectedFoilImage.indexOf("Green") >= 0)
				{
					FoilName = "GreenFoil";
				}
				else if (SelectedFoilImage.indexOf("Pink") >= 0)
				{
					FoilName = "PinkFoil";
				}
				else if (SelectedFoilImage.indexOf("Red") >= 0)
				{
					FoilName = "RedFoil";
				}
				else if (SelectedFoilImage.indexOf("Silver") >= 0)
				{
					FoilName = "SilverFoil";
				}
				
				if(Debugging) Alert.show('SelectFoilGalleryImage');
				if(Debugging) Alert.show('FoilName:');
				if(Debugging) Alert.show(FoilName);
			}
			
