			private function SetHolesDescription():void
			{
				if (SelectedHoles.indexOf("FourCups") >= 0)
				{
					Holes = 'FourCups'
				}
				else if (SelectedHoles.indexOf("TopCups") >= 0)
				{
					Holes = 'TopCups';
				}
				else if (SelectedHoles.indexOf("MiddleCups") >= 0)
				{
					Holes = 'MiddleCups';
				}
				else if (SelectedHoles.indexOf("BottomCups") >= 0)
				{
					Holes = 'BottomCups';
				}
				else if (SelectedHoles.indexOf("FourHoles") >= 0)
				{
					Holes = 'FourHoles';
				}
				else if (SelectedHoles.indexOf("TopHoles") >= 0)
				{
					Holes = 'TOpHoles';
				}
				else if (SelectedHoles.indexOf("MiddleHoles") >= 0)
				{
					Holes = 'MiddleHoles';
				}
				else if (SelectedHoles.indexOf("BottomHoles") >= 0)
				{
					Holes = 'BottomHoles';
				}
				else if (SelectedHoles.indexOf("NoneNone") >= 0)
				{
					Holes = 'NoHolesNoCups';
				}
			}
			
