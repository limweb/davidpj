			private function AdjustBoxWidths(Context:String):void
			{
				if (CenterSymbol == false)
				{
					if (Context == 'LeftFlag')
					{
						Box1.width = (Box1.width - FlagWidth);
					}
					else if (Context == 'RightFlag')
					{
						Box1.width = (Box1.width - FlagWidth);
					}
					else if (Context == 'LeftDate')
					{
						Box1.width = (Box1.width - DateWidth);
					}
					else if (Context == 'RightDate')
					{
						Box1.width = (Box1.width - DateWidth);
					}
					else if (Context == 'LeftSymbol')
					{
						Box1.width = (Box1.width - SymbolWidth);
					}
					else if (Context == 'RightSymbol')
					{
						Box1.width = (Box1.width - SymbolWidth);
					}
					else if (Context == '1ASymbol')
					{
						Box1.width = (Box1.width - SymbolWidth);
					}
					else if (Context == '1ASymbol')
					{
						Box1.width = (Box1.width - SymbolWidth);
					}
				}
				else if (CenterSymbol == true)
				{
					if ((CurrentBox.indexOf('1') >= 0) && (Box2.width = 0))
					{
						Box1.width = ((PlateElementsContainer.width / 2) - (SymbolWidth / 2));
						Box2.width = ((PlateElementsContainer.width / 2) - (SymbolWidth / 2));
					}
						
					if (Context == 'LeftFlag')
					{
						Box1.width = (Box1.width - FlagWidth);
					}
					else if (Context == 'RightFlag')
					{
						Box2.width = (Box2.width - FlagWidth);
					}
					else if (Context == 'LeftDate')
					{
						Box1.width = (Box1.width - DateWidth);
					}
					else if (Context == 'RightDate')
					{
						Box2.width = (Box2.width - DateWidth);
					}
					else if (Context == 'LeftSymbol')
					{
						Box1.width = (Box1.width - SymbolWidth);
					}
					else if (Context == 'RightSymbol')
					{
						Box2.width = (Box2.width - SymbolWidth);
					}
				}
			}
			
