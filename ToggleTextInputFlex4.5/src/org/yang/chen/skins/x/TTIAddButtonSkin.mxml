<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:ns="library://ns.adobe.com/flex/mx"
	alpha.disabled="0.7">
	
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata> 
	
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	
	<!-- Text-->
	<s:Label id="labelDisplay" top="5" bottom="5" left="5" right="5" focusEnabled="false"
			 color.down="0x333333" color="0x6d6cb2"
			 textDecoration.over="underline"
			 textDecoration.down="underline"
			 textAlign="center" 
			 verticalAlign="middle" 
			 alpha.disabled="0.5"
			 horizontalCenter="0" verticalCenter="0"> 
	</s:Label> 
</s:SparkSkin>
